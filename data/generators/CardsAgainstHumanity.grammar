{
"_name": "Cards Against Humanity Card Combinator",
"_description": "Picks a random pair of Cards Against Humanity cards. Users can provide their own cards: if there's an underscore in the input it will be used as a black card, otherwise as a white card",
"_triggers": ["cah", "cardsagainsthumanity"],
"start": "<$hasparams|<paramscheck>|<$file|CardsAgainstHumanityBlackCards.txt>>",
"paramscheck": "<$ifcontains|%_params|_|<customBlackCard>|<customWhiteCard>>",
"customBlackCard": "<$regexreplace|%_params|_+|<whitecard>>",
"customWhiteCard": "<$setvar|customwhitecard|%_params><$file|CardsAgainstHumanityBlackCards.txt>",
"whitecard": "<$hasvar|customwhitecard|<$var|customwhitecard|&storeandshow:lastwhitecard,bold><$removevar|customwhitecard>|<$file|CardsAgainstHumanityWhiteCards.txt|&storeandshow:lastwhitecard,bold>>"
}